package com.ar.echoafcavlapplication.Models;

import com.ar.echoafcavlapplication.Enums.DriverCardStatusEnum;

public class DriverCardStatus {
    private String name;
    private String family;
    private DriverCardStatusEnum status = DriverCardStatusEnum.NOT_FOUND;

    public DriverCardStatus(String name, String family, DriverCardStatusEnum status) {
        this.name = name;
        this.family = family;
        this.status = status;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getFamily() {
        return family;
    }

    public void setFamily(String family) {
        this.family = family;
    }

    public DriverCardStatusEnum getStatus() {
        return status;
    }

    public void setStatus(DriverCardStatusEnum status) {
        this.status = status;
    }
}
